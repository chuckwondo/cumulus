{
  "name": "@cumulus/common",
  "version": "1.13.3",
  "description": "Common utilities used across tasks",
  "keywords": [
    "GIBS",
    "CUMULUS",
    "NASA"
  ],
  "engines": {
    "node": ">=8.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/common",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "scripts": {
    "build": "true",
    "build-docs": "jsdoc2md --template templates/docs-API.hbs StepFunctions.js string.js URLUtils.js util.js > docs/API.md",
    "lint": "eslint .",
    "mocha": "mocha test/**/*.js",
    "mocha-coverage": "nyc mocha-webpack test/**/*.js",
    "mocha-junit": "npm run mocha -- --reporter mocha-junit-reporter --reporter-options mochaFile=test-results.xml",
    "test": "npm run mocha && ava",
    "test-coverage": "npm run mocha && nyc ava",
    "test-ci": "npm run lint && npm run mocha-junit"
  },
  "ava": {
    "files": "tests"
  },
  "nyc": {
    "exclude": [
      "tests",
      "test"
    ]
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/checksum": "1.13.3",
    "@cumulus/logger": "1.13.3",
    "ajv": "^5.2.2",
    "async": "^2.0.0",
    "aws-sdk": "^2.238.1",
    "expect.js": "^0.3.1",
    "follow-redirects": "^1.2.4",
    "formidable": "^1.2.1",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.1",
    "got": "^9.2.1",
    "js-yaml": "^3.11.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.compact": "^3.0.1",
    "lodash.curry": "^4.1.1",
    "lodash.flowright": "^3.5.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isnil": "^4.0.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "lodash.kebabcase": "^4.1.1",
    "lodash.omitby": "^4.6.0",
    "lodash.range": "^3.2.0",
    "mocha": "^5.0.4",
    "mocha-junit-reporter": "^1.11.1",
    "node-forge": "^0.7.1",
    "p-limit": "^2.0.0",
    "p-map": "^1.2.0",
    "p-retry": "^2.0.0",
    "pump": "^3.0.0",
    "randexp": "^0.4.9",
    "url-join": "^4.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@cumulus/test-data": "1.13.3",
    "ava": "^0.25.0",
    "jsdoc-to-markdown": "^4.0.1",
    "nock": "^10.0.0",
    "nyc": "^14.0.0",
    "sinon": "^7.1.1"
  }
}
