{
  "name": "@cumulus/cmrjs",
  "version": "1.13.2",
  "description": "A node SDK for CMR",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "test": "ava",
    "test-coverage": "nyc ava",
    "debug": "NODE_ENV=test node --inspect-brk node_modules/ava/profile.js --serial tests/*.js"
  },
  "ava": {
    "files": "tests/**/*.js",
    "serial": true
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  },
  "keywords": [
    "CUMULUS"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/cmr-client": "1.13.2",
    "@cumulus/common": "1.13.2",
    "got": "^8.3.0",
    "js2xmlparser": "^4.0.0",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.property": "^4.4.2",
    "lodash.set": "^4.3.2",
    "public-ip": "^2.3.5",
    "url-join": "^1.0.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "fs-extra": "7.0.0",
    "lodash.some": "^4.6.0",
    "nock": "^10.0.2",
    "nyc": "^14.0.0",
    "rewire": "^4.0.1",
    "sinon": "^7.1.1"
  }
}
